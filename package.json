{
  "name": "@widcardw/rehype-asciimath",
  "type": "module",
  "private": "true",
  "version": "0.0.1",
  "description": "Rehype plugin that transforms both KaTeX and Asciimath into HTML",
  "scripts": {
    "test": "vitest",
    "build": "tsup ./src/index.ts --format cjs,esm --dts --clean",
    "release": "pnpm run build && bumpp && pnpm publish --access public"
  },
  "author": "widcardw <widcardw@foxmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/widcardw/rehype-asciimath#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/widcardw/rehype-asciimath.git"
  },
  "bugs": "https://github.com/widcardw/rehype-asciimath/issues",
  "keywords": [
    "rehype",
    "rehype-plugin",
    "asciimath"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "asciimath-parser": "^0.6.3",
    "hast-util-from-html-isomorphic": "^2.0.0",
    "hast-util-to-text": "^4.0.0",
    "katex": "^0.16.9",
    "unist-util-visit-parents": "^6.0.1"
  },
  "devDependencies": {
    "@types/hast": "^3.0.2",
    "@types/katex": "^0.16.5",
    "bumpp": "^9.2.0",
    "rehype-parse": "^9.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-math": "^6.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.0.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "unified": "^11.0.3",
    "vfile": "^6.0.1",
    "vitest": "^0.34.6"
  }
}
